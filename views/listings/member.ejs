    <!-- Replace all social link text with icons -->
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const socialLinks = document.querySelectorAll('.social-links');
            socialLinks.forEach(container => {
                const links = container.querySelectorAll('a');
                links.forEach(link => {
                    if (link.textContent.includes('LinkedIn')) {
                        link.innerHTML = '<i class="fab fa-linkedin"></i>';
                        link.setAttribute('title', 'LinkedIn Profile');
                    }
                    if (link.textContent.includes('GitHub')) {
                        link.innerHTML = '<i class="fab fa-github"></i>';
                        link.setAttribute('title', 'GitHub Profile');
                    }
                });
            });
        });
    </script>

